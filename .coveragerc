[run]
source = chat
omit =
    */migrations/*
    */tests/*
    */admin.py
    */apps.py
    */management/commands/*
    */wsgi.py
    */asgi.py
    */settings.py
    */urls.py
    manage.py

[report]
exclude_lines =
    pragma: no cover
    def __str__
    def __repr__
    raise NotImplementedError
    if settings.DEBUG
    pass
    raise ImportError
    except ImportError
    if 0:
    if __name__ == .__main__.:

[html]
directory = htmlcov
